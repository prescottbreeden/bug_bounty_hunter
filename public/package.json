    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch:sass": "node-sass src/sass/main.scss src/styles.css -w",
    "compile:sass": "node-sass src/sass/main.scss src/sass/comp/styles.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/sass/comp/styles.comp.css -o src/sass/comp/styles.prefixed.css",
    "compress:css": "node-sass src/sass/comp/styles.prefixed.css src/styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.4",
